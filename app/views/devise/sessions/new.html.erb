<p class="notice"><%= notice %></p>
  <p class="alert"><%= alert %></p>
<div class="box">
  <div class="box-inner">
    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <%= render "devise/shared/error_messages", resource: resource %>
   
   
      <h1>ログイン</h1>
      <div class="box-email">
        <%= f.label :email, "メールアドレス" %><br />
        <%= f.email_field :email, autofocus: true, autocomplete: "email", placeholder: "メールアドレス" %>
      </div>
      <div class="box-password">
        <div class="a-row">
          <div class="column-left">
            <%= f.label :password, "パスワード" %>
          </div>
          <div class="column-right">
           
          </div>
        </div>
        <%= f.password_field :password, autocomplete: "current-password", placeholder: "必須" %>
      </div>
      <div class="box-login">
        <%= f.submit "ログイン" %>
       
        <% if devise_mapping.rememberable? %>
          <div class="remember">
            <%= f.check_box :remember_me %>
            <%= f.label :remember, "ログインしたままにする" %>
          </div>
        <% end %>
      </div>
    <% end %>

    <div class="box-newusr">
      <p><span>------------</span> 初めてプログラミンナをご利用ですか？ <span>------------</span></p>
      <%= render "devise/shared/links" %>
    </div>
  </div>
</div>
